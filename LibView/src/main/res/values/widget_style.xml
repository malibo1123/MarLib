<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RedTextView">
        <attr name="inner_circle_color" format="color"/>
        <attr name="outer_oval_color" format="color"/>
        <attr name="inner_circle_radius" format="dimension"/>
        <attr name="outer_oval_width" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ViewPagerDotIndicator">
        <attr name="radius" format="integer" />
        <attr name="dotSpacing" format="integer" />
        <attr name="circleColor" format="color" />
    </declare-styleable>
    <declare-styleable name="VerticalSwitchTextView">
        <attr name="switchDuaration" format="integer"/>
        <attr name="idleDuaration" format="integer"/>
        <attr name="alignment">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="switchOrientation">
            <enum name="up"  value="0"/>
            <enum name="down"  value="1"/>
        </attr>
        <attr name="stringList" format="string"/>
        <attr name="sameDirection" format="boolean"/>
        <attr name="verticalSwitchStyle" >
            <enum name="inAndOut" value="1"/>
            <enum name="firstOutThenIn" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SwitchLayout">
        <attr name="switchTime" format="integer"/>
        <attr name="idleTime" format="integer"/>
        <attr name="switchDirection">
            <enum name="toLeft" value="1"/>
            <enum name="toRight" value="2"/>
            <enum name="toTop" value="3"/>
            <enum name="toBottom" value="4"/>
        </attr>
        <attr name="switchPlayStyle">
            <enum name="inAndOutSameTime"  value="1"/>
            <enum name="outThenInSameDirection"  value="2"/>
            <enum name="outThenInDiffDirection"  value="3"/>
        </attr>
        <attr name="switchInterpolator">
            <enum name="linearInterpolator"  value="1"/>
            <enum name="accelerateInterpolator"  value="2"/>
            <enum name="decelerateInterpolator"  value="3"/>
            <enum name="accelerateDecelerateInterpolator"  value="4"/>
            <enum name="anticipateInterpolator"  value="5"/>
            <enum name="bounceInterpolator"  value="6"/>
        </attr>
    </declare-styleable>

    <!--此自定义属性为自定义控件NameValueText使用 -->
    <declare-styleable name="CustomNameValueText">
        <attr name="nameText" format="string"/>
        <attr name="valueText" format="string"/>
        <attr name="nameTextSize" format="integer"/>
        <attr name="valueTextSize" format="integer"/>
        <attr name="nameTextColor" format="color"/>
        <attr name="valueTextColor" format="color"/>
        <attr name="nameLinePosition">
            <enum name="under" value="8" />
            <enum name="middle" value="16" />
            <enum name="bold" value="32"/>
        </attr>
        <attr name="valueLinePosition">
            <enum name="under" value="8" />
            <enum name="middle" value="16" />
            <enum name="bold" value="32"/>
        </attr>
        <attr name="defaultTextSize" format="integer"/>
        <attr name="defaultTextColor" format="color"/>
        <attr name="valueDrawableRight" format="reference"/>
        <attr name="valueDrawableRightPadding" format="dimension"/>
    </declare-styleable>

    <!--此自定义属性为自定义控件IconTextView使用 <attr name="myTextColorSelector" format="reference"/>
        <attr name="textClicked" format="color"/>
        <attr name="iconClicked" format="reference"/>-->
    <declare-styleable name="IconTextView">
        <attr name="textValue" format="string" />
        <attr name="textSize" format="integer"/>
        <attr name="textColor" format="color"/>
        <attr name="myIcon" format="reference"/>
        <attr name="iconSize" format="dimension"/>
        <attr name="iconPosition">
            <enum name="iconUp" value="0" />
            <enum name="iconDown" value="1" />
            <enum name="iconLeft" value="2"/>
            <enum name="iconRight" value="3"/>
        </attr>
        <attr name="textIconMargin" format="dimension"/>
    </declare-styleable>

    <!--此自定义属性为自定义控件NameValueEdit使用 -->
    <declare-styleable name="NameValueEdit">
        <attr name="name" format="string" />
        <attr name="nameSize" format="integer"/>
        <attr name="nameColor" format="color"/>
        <attr name="value" format="string" />
        <attr name="valueHint" format="string"/>
        <attr name="valueSize" format="integer"/>
        <attr name="valueColor" format="color"/>
        <attr name="hintColor" format="color"/>
        <attr name="rightIcon" format="reference"/>
        <attr name="nameValueMargin" format="dimension"/>
        <attr name="valueEditAble" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CustomPatternLockView">
        <attr name="lock_size" format="integer" />
        <attr name="lock_nodeSize" format="dimension" />
        <attr name="lock_nodeOnAnim" format="reference" />
        <attr name="lock_padding" format="dimension" />
        <attr name="lock_spacing" format="dimension" />
        <attr name="lock_enableVibrate" format="boolean" />
        <attr name="lock_vibrateTime" format="integer" />
        <attr name="lock_nodeSrc" format="color|reference" />
        <attr name="lock_nodeHighlightSrc" format="color|reference" />
        <attr name="lock_nodeCorrectSrc" format="color|reference" />
        <attr name="lock_nodeErrorSrc" format="color|reference" />
        <attr name="lock_lineColor" format="color" />
        <attr name="lock_lineWidth" format="dimension" />
        <attr name="lock_nodeTouchExpand" format="dimension" />
        <attr name="lock_autoLink" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RoundedLetterView">
        <attr name="titleText" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleColor" format="color" />
        <attr name="backgroundColorValue" format="color" />
    </declare-styleable>
    <declare-styleable name="RingProgressBar">
        <attr name="ringColor" format="color" />
        <attr name="ringProgressColor" format="color" />
        <attr name="ringWidth" format="dimension" />
        <attr name="ringTextColor" format="color" />
        <attr name="ringTextSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsShow" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="EmbellishTitle">
        <attr name="embellishTitleText" format="string" />
        <attr name="embellishTitleLeft" format="string" />
        <attr name="embellishTitleRight" format="string" />
        <attr name="embellishTitleTextCenterColor" format="color" />
        <attr name="embellishTitleTextSidesColor" format="color" />
        <attr name="embellishTitleTextSize" format="dimension" />
        <attr name="embellishTitleIcon" format="reference" />
    </declare-styleable>
    <declare-styleable name="ShadowImageView">
        <attr name="shadowRound" format="dimension" />
        <attr name="shadowSrc" format="reference" />
        <attr name="shadowColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CircleView">
        <attr name="cv_titleText" format="string" />
        <attr name="cv_subtitleText" format="string" />
        <attr name="cv_titleSize" format="dimension" />
        <attr name="cv_subtitleSize" format="dimension" />
        <attr name="cv_titleSubtitleSpace" format="float" />
        <attr name="cv_strokeWidthSize" format="float" />
        <attr name="cv_fillRadius" format="float" />
        <attr name="cv_titleColor" format="color" />
        <attr name="cv_subtitleColor" format="color" />
        <attr name="cv_fillColor" format="color" />
        <attr name="cv_strokeColorValue" format="color" />
        <attr name="cv_backgroundColorValue" format="color" />
    </declare-styleable>

    <declare-styleable name="refresh">
        <attr name="refresh_line_color" format="color" />
        <attr name="refresh_line_width" format="integer" />
    </declare-styleable>

    <declare-styleable name="download">
        <attr name="download_line_color" format="color" />
        <attr name="download_bg_line_color" format="color" />
        <attr name="download_text_color" format="color" />
        <attr name="download_line_width" format="integer" />
        <attr name="download_bg_line_width" format="integer" />
        <attr name="download_text_size" format="integer" />
    </declare-styleable>

    <declare-styleable name="play">
        <attr name="play_line_color" format="color" />
        <attr name="play_bg_line_color" format="color" />
        <attr name="play_line_width" format="integer" />
        <attr name="play_bg_line_width" format="integer" />
    </declare-styleable>

    <declare-styleable name="scroll">
        <attr name="scroll_line_color" format="color" />
        <attr name="scroll_line_width" format="integer" />
        <attr name="scroll_bg_line_color" format="color" />
        <attr name="scroll_bg_line_width" format="integer" />
        <attr name="scroll_is_selected" format="boolean" />
        <attr name="scroll_view_translation_x" format="integer" />
    </declare-styleable>

    <declare-styleable name="search">
        <attr name="search_line_color" format="color" />
        <attr name="search_line_width" format="integer" />
        <attr name="search_dot_size" format="integer" />
    </declare-styleable>

    <declare-styleable name="volume">
        <attr name="volume_line_color" format="color" />
        <attr name="volume_line_width" format="integer" />
        <attr name="volume_bg_line_color" format="color" />
        <attr name="volume_bg_line_width" format="integer" />
    </declare-styleable>
    <declare-styleable name="FlipImageView">
        <attr name="isAnimated" format="boolean" />
        <attr name="isFlipped" format="boolean" />
        <attr name="flipDrawable" format="reference" />
        <attr name="flipDuration" format="integer" />
        <attr name="flipInterpolator" format="reference" />
        <attr name="flipRotations">
            <flag name="none" value="0" />
            <flag name="x" value="1" />
            <flag name="y" value="2" />
            <flag name="z" value="4" />
        </attr>
        <attr name="reverseRotation" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="SVGAImageView">
        <attr name="source" format="string" />
        <attr name="autoPlay" format="boolean" />
        <attr name="antiAlias" format="boolean" />
        <attr name="loopCount" format="integer" />
        <attr name="clearsAfterStop" format="boolean" />
        <attr name="fillMode" format="enum">
            <enum name="Backward" value="0" />
            <enum name="Forward" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HVScrollView">
        <attr format="boolean" name="fillViewportH"/>
        <attr format="boolean" name="fillViewportV"/>
        <attr name="scrollOrientation">
            <enum name="none" value="0"/>
            <enum name="horizontal" value="1"/>
            <enum name="vertical" value="2"/>
            <enum name="both" value="3"/>
        </attr><attr format="boolean" name="childLayoutCenter"/>
    </declare-styleable>
</resources>
